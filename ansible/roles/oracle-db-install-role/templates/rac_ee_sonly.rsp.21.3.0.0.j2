oracle.install.responseFileVersion=/oracle/install/rspfmt_dbinstall_response_schema_v21.0.0
oracle.install.option=INSTALL_DB_SWONLY
UNIX_GROUP_NAME=oinstall
SELECTED_LANGUAGES=all_langs
INVENTORY_LOCATION={{ gv_inventory_loc }}
ORACLE_HOME={{ gv_oracle_home_db }}
ORACLE_BASE={{ gv_oracle_base }}
oracle.install.db.InstallEdition=EE
oracle.install.db.OSDBA_GROUP=dba
oracle.install.db.OSOPER_GROUP=oper
oracle.install.db.OSBACKUPDBA_GROUP=oinstall
oracle.install.db.OSDGDBA_GROUP=oinstall
oracle.install.db.OSKMDBA_GROUP=oinstall
oracle.install.db.OSRACDBA_GROUP=dba
oracle.install.db.CLUSTER_NODES={% for host in groups[hostgroup] -%} {{ hostvars[host].node_host_name }}{{ "," if not loop.last else "" }} {%- endfor +%}
oracle.install.db.isRACOneInstall=false 
oracle.install.db.rac.serverpoolCardinality=0 
oracle.install.db.config.starterdb.type=GENERAL_PURPOSE
oracle.install.db.ConfigureAsContainerDB=false
SECURITY_UPDATES_VIA_MYORACLESUPPORT=false
DECLINE_SECURITY_UPDATES=true
oracle.installer.autoupdates.option=SKIP_UPDATES
